  ";"}], "\[IndentingNewLine]", 
 RowBox[{"SetAttributes", "[", 
  RowBox[{"MiniMaxTTT", ",", "HoldFirst"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MiniMaxTTT", "[", 
   RowBox[{"ttt_Symbol", ",", "depth_", ",", "maxQ_", ",", 
    RowBox[{"maxDepth_:", "9"}]}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "nextBoard", ",", "bestMove", ",", "move", ",", "\[IndentingNewLine]", 
      RowBox[{"availableMoves", "=", 
       RowBox[{"AvailableMoves", "[", "ttt", "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"i", "=", "1"}], ",", "bestValue", ",", "value"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"TicTacToe", "[", 
      RowBox[{"\"\<winCheck\>\"", ",", "ttt"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"ttt", "[", "\"\<won\>\"", "]"}], "===", "False"}], " ", "||", 
        RowBox[{"availableMoves", "=!=", "False"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{"maxQ", ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"bestValue", "=", 
           RowBox[{"Prepend", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"-", "1000"}], "}"}], ",", 
             RowBox[{
             "availableMoves", "\[LeftDoubleBracket]", "i", 
              "\[RightDoubleBracket]"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"i", "=", "1"}], ",", 
            RowBox[{"i", "\[LessEqual]", 
             RowBox[{"Length", "[", "availableMoves", "]"}]}], ",", 
            RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"nextBoard", "=", "ttt"}], ";", "\[IndentingNewLine]", 
             RowBox[{"TicTacToe", "[", 
              RowBox[{"\"\<move\>\"", ",", "nextBoard", ",", 
               RowBox[{"Evaluate", "[", 
                RowBox[{
                "availableMoves", "\[LeftDoubleBracket]", "i", 
                 "\[RightDoubleBracket]"}], "]"}]}], "]"}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"value", "=", 
              RowBox[{"Prepend", "[", 
               RowBox[{
                RowBox[{"MiniMaxTTT", "[", 
